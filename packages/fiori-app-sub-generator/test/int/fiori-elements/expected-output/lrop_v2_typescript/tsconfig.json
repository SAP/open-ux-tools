{
    "compilerOptions": {
        "module": "es2022",
        "skipLibCheck": true,
        "allowJs": true,
        "strict": true,
        "strictPropertyInitialization": false,
        "moduleResolution": "node",
        "rootDir": "./webapp",
        "outDir": "./dist",
        "baseUrl": "./",
        "paths": {
            "testNameSpace/lropv2typescript/*": [
                "./webapp/*"
            ]
        },
        "typeRoots": [
            "./node_modules/@types",
            "./node_modules/@sapui5/ts-types-esm"
        ]
    },
    "include": [
        "./webapp/**/*"
    ]
}