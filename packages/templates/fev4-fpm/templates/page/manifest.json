{
    "sap.ui5": {
        "routing": {
            "targets": {
                "<%= entity %><%= name %>": {
					"type": "Component",
					"id": "<%= id %>",
					"name": "sap.fe.core.fpm",
					"options": {
						"settings": {
							"viewName": "<%= id %>.<%= name %>",
							"entitySet": "<%= entity %>",
							"navigation": { }
						}
					}
				}<%if (locals.navigation) {%>,
                "<%= navigation.sourcePage %>": {
                    "options": {
                        "settings": {
                            "navigation": {
                                "<%= navigation.navEntity %>": {
                                    "detail": {
                                        "route": "<%= entity %><%= name %>"
                                    }
                                }
                            }
                        }
                    }
                }<% } %>
            }
        }
    }
}