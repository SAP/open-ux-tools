{
    "sap.ui5": {
        "routing": {
            "routes": [
                {
					"pattern": ":?query:",
					"name": "<%= view.name %>",
					"target": "<%= view.name %>"
				}
            ],
            "targets": {
                "<%= view.name %>": {
                    "viewId": "<%= view.name %>",
                    "viewName": "<%= app.id %>.ext.view.<%= view.name %>",
                    "options": {
                        "settings": {
                            "entitySet": "<%= view.name %>",
                            "navigation": {}
                        }
                    }
                }
            }
        }
    }
}