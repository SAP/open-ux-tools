{
    "sap.ui5": {
        "routing": {
            "routes": [
                {
					"pattern": ":?query:",
					"name": "customPage",
					"target": "customPage"
				}
            ],
            "targets": {
                "customPage": {
                    "type": "Component",
                    "id": "customPage",
                    "name": "sap.fe.core.fpm",
                    "options": {
                        "settings": {
                            "viewName": "sap.fe.core.fpmExplorer.customPageContent.CustomPage",
                            "entitySet": "RootEntity",
                            "navigation": {
                                "RootEntity": {
                                    "detail": {
                                        "route": "objectPage"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}