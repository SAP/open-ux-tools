<mvc:View
	controllerName="{{2masterdetail.parameters.AppId.value}}.controller.NotFound"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">

	<MessagePage
		id="page"
		title="{i18n>notFoundTitle}"
		text="{i18n>notFoundText}"
		icon="sap-icon://document"
		{{#unless 2masterdetail.parameters.FLP.value.value}}
		showNavButton="true"
		navButtonPress=".onNavBack"{{/unless}}>
	</MessagePage>

</mvc:View>