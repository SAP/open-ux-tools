<mvc:View
	controllerName="{{2masterdetail.parameters.AppId.value}}.controller.DetailObjectNotFound"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc">

	<MessagePage
		id="page"
		title="{i18n>detailTitle}"
		text="{i18n>noObjectFoundText}"
		icon="sap-icon://product"
		description=""
		{{#unless 2masterdetail.parameters.FLP.value.value}}
		showNavButton="{=
			${device>/system/phone} ||
			${device>/system/tablet} &amp;&amp;
			${device>/orientation/portrait}
		}"
		navButtonPress=".onNavBack"{{/unless}}>
	</MessagePage>

</mvc:View>