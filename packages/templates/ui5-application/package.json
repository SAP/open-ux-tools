{
    "name": "@sap/ux-ui5-application-template",
    "version": "0.0.1",
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "scripts": {
        "build": "pnpm clean && tsc",
        "clean": "rimraf dist test/test-output",
        "format": "prettier --write '**/*.{js,json,ts,yaml,yml}' --ignore-path ../../../.prettierignore",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "test": "jest --ci --forceExit --detectOpenHandles --colors",
        "test-u": "jest --ci --forceExit --detectOpenHandles --colors -u",
        "watch": "tsc --watch",
        "link": "pnpm link --global",
        "unlink": "pnpm unlink --global"
    },
    "files": [
        "LICENSE",
        "dist",
        "templates"
    ],
    "dependencies": {
        "ejs": "3.1.6",
        "mem-fs": "2.1.0",
        "mem-fs-editor": "9.0.0"
    },
    "devDependencies": {
        "@types/ejs": "3.0.6",
        "@types/mem-fs": "1.1.2",
        "@types/mem-fs-editor": "7.0.0",
        "typescript": "4.2.4"
    },
    "engines": {
        "pnpm": ">=6.0.2",
        "node": ">=12.18.2 < 13.0.0 || > 14.0.0 < 15.0.0 || >=16.1.0 < 17.0.0"
    }
}
