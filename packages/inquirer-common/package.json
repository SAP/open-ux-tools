{
    "name": "@sap-ux/inquirer-common",
    "description": "Commonly used shared functionality and types to support inquirer modules.",
    "version": "0.4.9",
    "repository": {
        "type": "git",
        "url": "https://github.com/SAP/open-ux-tools.git",
        "directory": "packages/inquirer-common"
    },
    "bugs": {
        "url": "https://github.com/SAP/open-ux-tools/issues?q=is%3Aopen+is%3Aissue+label%3Abug+label%3Ainquirer-common"
    },
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc --build",
        "clean": "rimraf --glob dist test/test-output coverage *.tsbuildinfo",
        "watch": "tsc --watch",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "test": "jest --ci --forceExit --detectOpenHandles --colors --passWithNoTests",
        "test-u": "jest --ci --forceExit --detectOpenHandles --colors -u",
        "link": "pnpm link --global",
        "unlink": "pnpm unlink --global"
    },
    "files": [
        "LICENSE",
        "dist",
        "!dist/*.map",
        "!dist/**/*.map"
    ],
    "dependencies": {
        "@sap-ux/ui5-info": "workspace:*",
        "fuzzy": "0.1.3",
        "i18next": "23.5.1",
        "chalk": "4.1.2",
        "figures": "3.2.0",
        "semver": "7.5.4",
        "lodash": "4.17.21"
    },
    "devDependencies": {
        "@sap-devx/yeoman-ui-types": "1.14.4",
        "@types/inquirer": "8.2.6",
        "@types/semver": "7.5.4",
        "@types/lodash": "4.14.202"
    },
    "engines": {
        "node": ">=18.x"
    }
}
