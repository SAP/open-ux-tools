{
    "compilerOptions": {
        "target": "es2022",
        "module": "es2022",
        "skipLibCheck": true,
        "allowJs": true,
        "strict": true,
        "strictPropertyInitialization": false,
        "moduleResolution": "node",
        "rootDir": "./webapp",
        "outDir": "./dist",
        "baseUrl": "./",
        "paths": {
            "<%-app.id.replace(/\./g, '/') %>/*": [
                "./webapp/*"
            ]
        },
        "typeRoots": [
            "./node_modules/@types",
            "./node_modules/<%- ui5.typesPackage %>"
        ]
    },
    "include": [
        "./webapp/**/*"
    ]
}