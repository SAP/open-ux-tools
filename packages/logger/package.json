        "winston": "3.7.2",
        "winston-transport": "4.5.0"
    },
    "devDependencies": {
        "@types/debug": "4.1.8",
        "@types/lodash": "4.14.198",
        "@types/vscode": "1.73.1",
        "jest-extended": "3.2.4",
        "jest": ">=29.5.0",
        "logform": "2.3.2"
    },
    "files": [
        "dist",
        "LICENSE",
        "!dist/*.map",
        "!dist/**/*.map"
    ],
    "engines": {
        "pnpm": ">=6.26.1 < 7.0.0 || >=7.1.0",
        "node": ">= 14.16.0 < 15.0.0 || >=16.1.0 < 17.0.0 || >=18.0.0 < 19.0.0"
    }
