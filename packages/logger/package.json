{
    "name": "@sap-ux/logger",
    "version": "0.8.0",
    "description": "Module with utils common in Fiori tools",
    "license": "SEE LICENSE IN LICENSE",
    "private": false,
    "main": "dist/index.js",
    "scripts": {
        "build": "pnpm clean && tsc",
        "watch": "tsc --watch",
        "clean": "rimraf dist coverage",
        "format": "prettier --write '**/*.{js,json,ts,yaml,yml}' --ignore-path ../../.prettierignore",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "test": "jest --maxWorkers=1 --colors",
        "test-u": "jest --maxWorkers=1 --colors -u",
        "link": "pnpm link --global",
        "unlink": "pnpm unlink --global"
    },
    "dependencies": {
        "lodash.clonedeep": "4.5.0",
        "logform": "2.3.2",
        "winston": "3.3.3",
        "winston-transport": "4.4.1"
    },
    "devDependencies": {
        "@types/debug": "4.1.7",
        "@types/lodash.clonedeep": "4.5.6",
        "@types/vscode": "1.63.1",
        "jest-extended": "1.2.0"
    },
    "files": [
        "dist",
        "LICENSE"
    ],
    "engines": {
        "pnpm": ">=6.0.2",
        "node": ">=12.22.5 < 13.0.0 || >= 14.16.0 < 15.0.0 || >=16.1.0 < 17.0.0"
    }
}
