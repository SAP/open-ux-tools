{
  "name": "v2lrop",
  "version": "0.0.1",
  "private": true,
  "sapux": true,
  "description": "An SAP Fiori application.",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "main": "webapp/index.html",
  "scripts": {
    "start": "fiori run --open test/flpSandbox.html?sap-client=100#masterDetail-display --config ui5.yaml",
    "start-mock": "fiori run --open test/flpSandboxMockServer.html?sap-client=100#masterDetail-display",
    "start-local": "fiori run --config ./ui5-local.yaml --open test/flpSandboxMockServer.html?sap-client=100#masterDetail-display",
    "build": "rimraf dist && ui5 build -a --include-task=generateCachebusterInfo",
    "deploy": "fiori add deploy-config"
  },
  "remarkConfig": {
    "plugins": [
      "remark-preset-lint-consistent"
    ]
  },
  "devDependencies": {
    "rimraf": "3.0.0",
    "@ui5/cli": "^2.14.1",
    "@ui5/fs": "^2.0.6",
    "@ui5/logger": "^2.0.1",
    "@sap/ux-specification": "1",
    "@sap/ux-ui5-tooling": "1"
  },
  "ui5": {
    "dependencies": [
      "@sap/ux-ui5-tooling"
    ]
  }
}
