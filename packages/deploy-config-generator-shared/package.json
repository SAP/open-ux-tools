{
    "name": "@sap-ux/deploy-config-generator-shared",
    "version": "0.0.0",
    "description": "Library for testing",
    "repository": {
        "type": "git",
        "url": "https://github.com/SAP/open-ux-tools.git",
        "directory": "packages/deploy-config-generator-shared"
    },
    "bugs": {
        "url": "https://github.com/SAP/open-ux-tools/issues?q=is%3Aopen+is%3Aissue+label%3Abug+label%3Adeploy-config-generator-shared"
    },
    "license": "Apache-2.0",
    "private": false,
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc --build",
        "watch": "tsc --watch",
        "clean": "rimraf --glob dist coverage *.tsbuildinfo",
        "format": "prettier --write '**/*.{js,json,ts,yaml,yml}' --ignore-path ../../.prettierignore",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "test": "jest --ci --forceExit --detectOpenHandles --colors",
        "test-u": "jest --ci --forceExit --detectOpenHandles --colors -u",
        "link": "pnpm link --global",
        "unlink": "pnpm unlink --global"
    },
    "dependencies": {
        "@types/yeoman-test": "4.0.6",
        "@sap-ux/fiori-generator-shared": "workspace:*",
        "@sap-devx/yeoman-ui-types": "1.14.4",
        "@vscode-logging/logger": "2.0.0",
        "i18next": "20.6.1",
        "yeoman-generator": "5.10.0",
        "yeoman-test": "6.3.0"
    },
    "devDependencies": {
        "@types/yeoman-generator": "5.2.11",
        "typescript": "5.3.3"
    },
    "files": [
        "dist",
        "LICENSE",
        "!dist/*.map",
        "!dist/**/*.map"
    ],
    "engines": {
        "node": ">=18.x"
    }
}
