{
    "name": "@sap-ux/axios-extension",
    "version": "0.13.2",
    "description": "Extension of the Axios module adding convinience methods to interact with SAP systems especially with OData services.",
    "repository": {
        "type": "git",
        "url": "https://github.com/SAP/open-ux-tools.git",
        "directory": "packages/axios-extension"
    },
    "bugs": {
        "url": "https://github.com/SAP/open-ux-tools/issues?q=is%3Aopen+is%3Aissue+label%3Abug+label%3Aaxios-extension"
    },
    "license": "Apache-2.0",
    "private": false,
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc --build",
        "watch": "tsc --watch",
        "clean": "rimraf dist test/test-output coverage *.tsbuildinfo",
        "format": "prettier --write '**/*.{js,json,ts,yaml,yml}' --ignore-path ../../.prettierignore",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "test": "jest --ci --forceExit --detectOpenHandles --colors",
        "test-u": "jest --ci --forceExit --detectOpenHandles --colors -u",
        "link": "pnpm link --global",
        "unlink": "pnpm unlink --global"
    },
    "dependencies": {
        "@sap-ux/btp-utils": "workspace:*",
        "@sap-ux/logger": "workspace:*",
        "axios": "0.24.0",
        "detect-content-type": "1.2.0",
        "fast-xml-parser": "3.12.20",
        "lodash": "4.17.21",
        "open": "7.0.3",
        "qs": "6.7.3",
        "xpath": "0.0.32",
        "@xmldom/xmldom": "0.8.4"
    },
    "devDependencies": {
        "@types/lodash": "4.14.176",
        "nock": "13.2.1",
        "supertest": "6.3.2"
    },
    "files": [
        "dist",
        "LICENSE",
        "!dist/*.map",
        "!dist/**/*.map"
    ],
    "engines": {
        "pnpm": ">=6.26.1 < 7.0.0 || >=7.1.0",
        "node": ">= 14.16.0 < 15.0.0 || >=16.1.0 < 17.0.0 || >=18.0.0 < 19.0.0"
    }
}
