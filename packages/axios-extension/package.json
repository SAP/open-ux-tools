{
    "name": "@sap-ux/axios-extension",
    "version": "0.8.0",
    "description": "Extension of the Axios module adding convinience methods to interact with OData service especially services exposed by SAP systems.",
    "license": "SEE LICENSE IN LICENSE",
    "private": false,
    "main": "dist/index.js",
    "scripts": {
        "build": "pnpm clean && tsc",
        "watch": "tsc --watch",
        "clean": "rimraf dist coverage",
        "format": "prettier --write '**/*.{js,json,ts,yaml,yml}' --ignore-path ../../.prettierignore",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "test": "jest --ci --forceExit --colors",
        "test-u": "jest --ci --forceExit --colors -u",
        "link": "pnpm link --global",
        "unlink": "pnpm unlink --global"
    },
    "dependencies": {
        "@sap-ux/btp-utils": "workspace:*",
        "@sap-ux/logger": "workspace:*",
        "axios": "0.24.0",
        "express": "4.17.1",
        "fast-xml-parser": "3.12.20",
        "lodash.clonedeep": "4.5.0",
        "open": "7.0.3",
        "qs": "6.7.0"
    },
    "devDependencies": {
        "@types/lodash.clonedeep": "4.5.6",
        "nock": "13.2.1"
    },
    "files": [
        "dist",
        "LICENSE"
    ],
    "engines": {
        "pnpm": ">=6.0.2",
        "node": ">=12.22.5 < 13.0.0 || >= 14.16.0 < 15.0.0 || >=16.1.0 < 17.0.0"
    }
}
