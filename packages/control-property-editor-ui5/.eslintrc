{
    "extends": ["../../.eslintrc"],
    "parserOptions": {
        "project": "./tsconfig-lint.json"
    },
    "plugins": ["import"],
    "overrides": [
        {
            "files": ["**/src/**/*.ts", "**/test/**/*.ts"],
            "rules": {
                // ui5 modules are not available in file system
                "import/no-unresolved": ["error", { "ignore": ["^sap/"] }]
            }
        }
    ],
    "rules": {
        "no-console": "off"
    }
}
