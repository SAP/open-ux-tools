{
    "name": "@sap-ux/fiori-freestyle-writer",
    "version": "0.9.0",
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "scripts": {
        "build": "rimraf dist && tsc",
        "clean": "rimraf dist test/test-output coverage",
        "watch": "tsc --watch",
        "lint": "eslint . --ext .ts",
        "test": "jest --ci --forceExit --detectOpenHandles --colors",
        "test-u": "jest --ci --forceExit --detectOpenHandles --colors -u",
        "test:local": "node -r ts-node/register test/index.ts",
        "link": "pnpm link --global",
        "unlink": "pnpm unlink --global"
    },
    "files": [
        "dist",
        "templates"
    ],
    "dependencies": {
        "@sap-ux/odata-service-writer": "workspace:*",
        "@sap-ux/ui5-application-writer": "workspace:*",
        "@sap-ux/ui5-config": "workspace:*",
        "ejs": "^3.1.6",
        "i18next": "20.3.2",
        "lodash": "4.17.21",
        "mem-fs": "^2.1.0",
        "mem-fs-editor": "^9.0.0"
    },
    "devDependencies": {
        "@sap-ux/open-ux-tools-types": "workspace:*",
        "@types/ejs": "^3.0.6",
        "@types/fs-extra": "9.0.13",
        "@types/lodash": "4.14.175",
        "@types/mem-fs-editor": "^7.0.0",
        "typescript": "4.2.4",
        "fs-extra": "10.0.0"
    },
    "engines": {
        "pnpm": ">=6.0.2",
        "node": ">=12.18.2 < 13.0.0 || > 14.0.0 < 15.0.0 || >=16.1.0 < 17.0.0"
    }
}
