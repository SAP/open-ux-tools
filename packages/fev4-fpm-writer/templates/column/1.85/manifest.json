{
    "sap.ui5": {
        "routing": {
            "targets": {
                "<%= target %>": {
                    "options": {
                        "settings": {
                            "controlConfiguration": {
                                "<%= targetEntity %>": {
                                    "columns": {
                                        "<%= id %>": {
                                            "header": "<%= header %>",
                                            <% if (locals.width)  { %> 
                                                "width": "<%= width %>",  
                                            <% } %>
                                            "position": {
                                                <% if (position.placement) { %> 
                                                    "placement": "<%= position.placement %>", 
                                                <% } %>
                                                <% if (position.anchor) { %> 
                                                    "anchor": "<%= position.anchor %>" 
                                                <% } %>
                                            },
                                            "template": "<%= ns %>.<%- id %>"
                                            <% if (locals.availability) { %> 
                                                , "availability": "<%= availability %>" 
                                            <% } %>
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}