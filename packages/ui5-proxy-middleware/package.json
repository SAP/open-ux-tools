        "i18next": "25.3.0",
        "proxy-from-env": "1.1.0"
    },
    "devDependencies": {
        "@sap-ux/project-access": "workspace:*",
        "@types/express": "4.17.21",
        "@types/supertest": "2.0.12",
        "@types/proxy-from-env": "1.0.1",
        "express": "4.22.1",
        "nock": "13.4.0",
