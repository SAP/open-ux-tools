<mvc:View
    controllerName="<%=app.id%>.controller.<%= ui5.initialViewName ? ui5.initialViewName : 'View1' %>"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true"
    xmlns="sap.m"
>
	<Shell id="shell">
		<App id="app">
			<pages>
				<Page id="page" title="{i18n>title}">
					<content />
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>
