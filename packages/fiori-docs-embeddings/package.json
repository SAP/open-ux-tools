{
  "name": "@sap-ux/fiori-docs-embeddings",
  "version": "0.0.0",
  "description": "SAP Fiori documentation indexing",
  "private": false,
  "scripts": {
    "build-compiler": "tsc --noEmit",
    "build": "npm-run-all -l -s build-compiler",
    "update-docs": "tsx src/scripts/build-docs.ts",
    "prepublishOnly": "npm run update-embeddings",
    "update-embeddings": "tsx src/scripts/build-embeddings.ts",
    "update-all": "npm-run-all -l -s update-docs update-embeddings",
    "clean": "rm -rf dist data/embeddings",
    "test": "jest --passWithNoTests --silent",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "dependencies": {
    "@lancedb/lancedb": "0.21.3",
    "@xenova/transformers": "2.17.2"
  },
  "devDependencies": {
    "npm-run-all2": "8.0.4",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "nodemon": "^3.0.0",
    "node-fetch": "^3.3.2",
    "marked": "^12.0.0",
    "gray-matter": "^4.0.3",
    "zod": "^3.22.4",
    "tsx": "^4.7.0"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "sap",
    "fiori",
    "documentation",
    "indexing"
  ],
  "license": "Apache-2.0",
  "files": [
    "bin/**/*",
    "dist/**/*",
    "data/**/*",
    "README.md",
    "package.json",
    "LICENSE"
  ],
  "main": "index.js",
  "exports": {
    ".": "./index.js",
    "./data": "./data/index.js",
    "./data/*": "./data/*"
  }
}