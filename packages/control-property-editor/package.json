        "autoprefixer": "10.4.7",
        "body-parser": "1.20.3",
        "ejs": "3.1.10",
        "esbuild-plugin-alias": "0.2.1",
        "esbuild-plugin-copy": "2.1.1",
        "eslint-plugin-react": "7.37.5",
        "http-proxy-middleware": "2.0.9",
        "i18next": "25.3.0",
        "npm-run-all2": "6.2.0",
        "postcss": "8.5.1",
        "postcss-modules": "6.0.1",
        "react": "16.14.0",
        "react-dom": "16.14.0",
        "react-i18next": "15.4.1",
        "react-redux": "7.2.9",
        "redux": "4.0.4",
        "redux-logger": "3.0.6",
        "rimraf": "5.0.5",
        "source-map-support": "0.5.16",
        "stream-browserify": "3.0.0",
