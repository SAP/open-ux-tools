{
  "name": "@sap-ux/control-property-editor",
  "displayName": "Control Property Editor",
  "description": "Control Property Editor",
  "license": "SEE LICENSE IN LICENSE",
  "version": "1.0.0",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/SAP/open-ux-tools.git",
    "directory": "packages/control-property-editor"
},
  "private": false,
  "scripts": {
    "pre-commit": "lint-staged --quiet",
    "clean:dist": "rimraf ./dist ./generators *.tsbuildinfo",
    "clean": "rimraf ./out ./reports",
    "clean:all": "npm-run-all clean",
    "build": "npm-run-all -l -s clean -p build:compile build:webapp",
    "build:compile": "tsc  --build ./tsconfig.build.json --pretty --dry",
    "build:webapp": "node esbuild.js",
    "watch": "npm-run-all -p watch:*",
    "watch:webapp": "node esbuild.js --watch --minify=false",
    "start": "node development/server",
    "test": "jest --maxWorkers=1 --ci --forceExit --detectOpenHandles --config='jest.config.js'",
    "test:integration": "jest --maxWorkers=1 --ci --config='jest.ui5.config.js'",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand  --config='jest.config.js' --colors",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format:fix": "prettier --write --ignore-path ../../../.prettierignore",
    "format:fix:all": "prettier --write '**/*.{css,scss,html,js,json,ts,tsx,yaml,yml}' '!**/{out,dist,typings,node_modules}/**' '!**/*.{svg,png,xml}' --ignore-path ../../../.prettierignore",
    "madge": "madge --warning --circular --extensions ts ./",
    "setup": "./setup.sh"
  },
  "devDependencies": {
    "@fluentui/react": "8.100.0",
    "@fluentui/react-hooks": "8.6.14",
    "@reduxjs/toolkit": "1.6.1",
    "@sap-ux/ui-components": "workspace:*",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "12.1.4",
    "@testing-library/dom": "9.3.1",
    "@types/expect-puppeteer": "4.4.5",
    "@types/jest": "29.2.6",
    "@types/jest-dev-server": "5.0.0",
    "@types/puppeteer": "5.4.4",
    "@types/react-dom": "16.8.5",
    "@types/react-redux": "7.1.1",
    "@types/redux-logger": "3.0.7",
    "@types/remote-redux-devtools": "0.5.4",
    "@types/source-map-support": "0.5.0",
    "@types/vscode": "1.73.1",
    "body-parser": "1.20.1",
    "eslint-plugin-react": "7.32.2",
    "expect-puppeteer": "5.0.3",
    "http-proxy-middleware": "1.3.1",
    "i18next": "19.0.2",
    "jest-dev-server": "7.0.0",
    "jest-sonar": "0.2.12",
    "npm-run-all": "4.1.5",
    "puppeteer-core": "7.0.0",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "react-i18next": "11.10.0",
    "react-redux": "7.1.0",
    "@types/react": "16.14.45",
    "redux": "4.0.4",
    "redux-logger": "3.0.6",
    "rimraf": "3.0.0",
    "source-map-support": "0.5.16",
    "stream-browserify": "3.0.0",
    "ts-import-plugin": "1.6.3",
    "ts-jest": "29.1.1",
    "typescript": "4.9.5",
    "postcss-modules": "6.0.0",
    "jest-junit": "15.0.0"
  },
  "dependencies": {
    "@sap-ux/control-property-editor-common": "workspace:*"
  },
  "jestSonar": {
    "reportPath": "reports/test/unit",
    "reportFile": "test-report.xml"
  },
  "eslint-formatter-multiple": {
    "formatters": [
      {
        "name": "stylish",
        "output": "console"
      },
      {
        "name": "json",
        "output": "file",
        "path": "reports/lint/eslint.json"
      },
      {
        "name": "checkstyle",
        "output": "file",
        "path": "reports/lint/eslint.checkstyle.xml"
      }
    ]
  }
}