{
    "name": "@sap-ux/prompts-common",
    "description": "Shared prompting code",
    "version": "0.1.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/SAP/open-ux-tools.git",
        "directory": "packages/prompts-common"
    },
    "bugs": {
        "url": "https://github.com/SAP/open-ux-tools/issues?q=is%3Aopen+is%3Aissue+label%3Abug+label%3Aprompts-common"
    },
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc --build",
        "clean": "rimraf --glob dist test/test-output coverage *.tsbuildinfo",
        "watch": "tsc --watch",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "test": "jest --ci --forceExit --detectOpenHandles --colors",
        "test-u": "jest --ci --forceExit --detectOpenHandles --colors -u",
        "link": "pnpm link --global",
        "unlink": "pnpm unlink --global"
    },
    "files": [
        "LICENSE",
        "dist",
        "!dist/*.map",
        "!dist/**/*.map"
    ],
    "dependencies": {
        "@sap-devx/yeoman-ui-types": "1.12.0",
        "@sap-ux/btp-utils": "workspace:*",
        "@sap-ux/ui5-info": "workspace:*",
        "fuzzy": "0.1.3",
        "i18next": "23.5.1",
        "inquirer-autocomplete-prompt": "2.0.1",
        "validate-npm-package-name": "5.0.0",
        "semver": "7.5.4"
    },
    "devDependencies": {
        "@types/inquirer": "8.2.7",
        "@types/semver": "7.5.3",
        "@types/validate-npm-package-name": "4.0.1"
    },
    "engines": {
        "pnpm": ">=6.26.1 < 7.0.0 || >=7.1.0",
        "node": ">= 14.16.0 < 15.0.0 || >=16.1.0 < 17.0.0 || >=18.0.0 < 19.0.0"
    }
}
