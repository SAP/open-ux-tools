{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
      "moduleResolution": "node",
      "skipLibCheck": true,
      "allowJs": true,
      "strict": true,
      "strictPropertyInitialization": false,
      "rootDir": "./",
      "outDir": "./dist",
      "baseUrl": "./",
      "types": [
        "<%= tsTypes %>"
    ],
      "paths": {
          "<%= libraryNamespaceURI %>/*": [
              "./src/<%= libraryNamespaceURI %>/*"
          ]
      }
  },
  "include": [
      "./src/**/*",
      "./test/**/*"
  ]
}