{
    "name": "@sap/ux-ui5-config",
    "description": "Library to UI5 configuration artifacts",
    "version": "0.0.1",
    "main": "dist/index.js",
    "scripts": {
        "build": "pnpm clean && tsc",
        "clean": "rimraf dist",
        "format": "prettier --write '**/*.{js,json,ts,yaml,yml}' --ignore-path ../../../.prettierignore",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "test": "jest --passWithNoTests --runInBand --ci --forceExit --detectOpenHandles --colors",
        "watch": "tsc --watch"
    },
    "files": [
        "dist"
    ],
    "dependencies": {
        "@sap/ux-yaml": "0.0.1"
    }
}
