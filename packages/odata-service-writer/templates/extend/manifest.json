{
    "sap.app": {
        "dataSources": {
            "<%=name%>": {
                "uri": "<%=path%>",
                "type": "OData",
                "settings": {
                    "annotations": [<%if (locals.annotations && annotations.length > 0) {%>
                        <%- annotations.map(anno => '"' + anno.name + '"').join(',') %><% if (locals.localAnnotationsName) {%>,<%}%><% } %><% if (locals.localAnnotationsName) { %>
                        "<%- localAnnotationsName %>"<% } %>
                    ]<% if (locals.metadata) { %>,
                    "localUri": "localService/metadata.xml" <% } %><%if (version === '4') {%>,
                    "odataVersion": "4.0"<% } %><%if (version === '2') {%>,
                    "odataVersion": "2.0"<% } %>
                }
            }<%if (locals.annotations && annotations.length > 0) { for(var i=0; i<annotations.length; i++) {%>,
            "<%- annotations[i].name %>": {
                "uri": "/sap/opu/odata/IWFND/CATALOGSERVICE;v=2/Annotations(TechnicalName='<%- encodeURIComponent(annotations[i].technicalName) %>',Version='0001')/$value/",
                "type": "ODataAnnotation",
                "settings": {
                    "localUri": "localService/<%- annotations[i].technicalName %>.xml"
                }
            }
           <%} } %><% if (locals.localAnnotationsName) { %>,
            "<%- localAnnotationsName %>": {
                "type": "ODataAnnotation",
                "uri": "annotations/<%- localAnnotationsName %>.xml",
                "settings": {
                    "localUri": "annotations/<%- localAnnotationsName %>.xml"
                }
            } <% } %>
        }
    },
    "sap.ui5": {
        "models": {
            "<%=model%>": {
                "dataSource": "<%=name%>",
                "preload": true,
                "settings": {<%if (version === '4') {%>
                    <% if (locals.includeSynchronizationMode) { %>
                    "synchronizationMode": "None",
                    <% } %>
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true<% } %>
                }
            }
        }
    }
}
