{
  "compilerOptions": {
    "target": "es2022",
    "module": "es2022",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "rootDir": "./",
    "outDir": "./dist",
    "baseUrl": "./",
    "typeRoots": ["./node_modules/@types", "./node_modules/<%- typesPackage %>"],
    "paths": {
      "<%= id %>/*": ["./webapp/*"]
    }
  },
  "include": ["./webapp/**/*"]
}