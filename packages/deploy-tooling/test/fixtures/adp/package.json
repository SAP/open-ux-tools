{
    "name": "adp.v2app",
    "version": "0.0.1",
    "sapuxLayer": "VENDOR",
    "devDependencies": {
        "@ui5/cli": "^3.3.2",
        "@sap-ux/backend-proxy-middleware": "^0.6.68",
        "@sap-ux/preview-middleware": "^0.5.0",
        "@sap-ux/ui5-proxy-middleware": "^1.1.34"
    },
    "scripts": {
        "build": "ui5 build --exclude-task generateFlexChangesBundle generateComponentPreload --clean-dest",
        "start": "ui5 serve --open /test/flp.html#app-preview",
