{
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "preserveConstEnums": true,
    "sourceMap": false,
    "allowJs": true,
    "strict": true,
    "strictNullChecks": true,
    "strictPropertyInitialization": true,
    "rootDir": "./",
    "outDir": "./src-gen",
    "baseUrl": "./",
    "typeRoots": ["./node_modules/@types", "./node_modules/<%= tsTypes %>"],
    "paths": {
      "<%= libraryNamespaceURI %>/*": ["./src/<%= libraryNamespaceURI %>/*"]
    }
  },
  "references": [
    {
        "path": "../ui5-config"
    }
],
  "include": ["./src/**/*", "./test/**/*"]
}
