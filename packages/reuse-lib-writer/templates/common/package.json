{
  "name": "<%= libraryname %>",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "@ui5/cli": "^2.9.3",
    "karma": "^6.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-cli": "^2.0.0",
    "karma-ui5": "^2.3.3",
    "ui5-middleware-livereload": "^0.5.1"
  },
  "ui5": {
    "dependencies": [
      "ui5-middleware-livereload"
    ]
  },
  "scripts": {
    "build": "ui5 build --clean-dest",
    "start": "ui5 serve --open test-resources/<%= librarynamespaceURI %>/Example.html",
    "testsuite": "ui5 serve --open test-resources/<%= librarynamespaceURI %>/qunit/testsuite.qunit.html",
    "test": "karma start --browsers=ChromeHeadless --singleRun=true"
  },
  "license": "UNLICENSED"
}
