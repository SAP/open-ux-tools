{
  "compilerOptions": {
      "module": "es2022",
      "moduleResolution": "node",
      "skipLibCheck": true,
      "allowJs": true,
      "strict": true,
      "strictPropertyInitialization": false,
      "rootDir": "./",
      "outDir": "./dist",
      "baseUrl": "./",
      "typeRoots": [
        "./node_modules/@types",
        "./node_modules/@sapui5/ts-types-esm"
      ],
      "paths": {
          "com/sap/tslibrary1/*": [
              "./src/com/sap/tslibrary1/*"
          ]
      }
  },
  "include": [
      "./src/**/*",
      "./test/**/*"
  ]
}