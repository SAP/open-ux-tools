{
    "name": "@sap-ux-private/control-property-editor-common",
    "displayName": "Control Property Editor",
    "description": "A common module for Control Property Editor react app and ui5",
    "license": "Apache-2.0",
    "version": "0.1.0",
    "main": "dist/index.js",
    "repository": {
        "type": "git",
        "url": "https://github.com/SAP/open-ux-tools.git",
        "directory": "packages/control-property-editor-common"
    },
    "private": false,
    "scripts": {
        "clean:dist": "rimraf ./dist ./generators *.tsbuildinfo",
        "clean": "rimraf ./out ./reports ./.webpack_cache",
        "clean:all": "npm-run-all clean",
        "build": "npm-run-all -l -s clean -p build:compile",
        "build:compile": "tsc  --build ./tsconfig.build.json --pretty",
        "watch": "build:compile --watch",
        "test": "jest --maxWorkers=1 --ci --forceExit --detectOpenHandles --config='jest.config.js'",
        "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand  --config='jest.config.js' --colors",
        "lint": "eslint . --ext .ts,.tsx",
        "lint:fix": "eslint . --ext .ts,.tsx --fix"
    },
    "devDependencies": {
        "npm-run-all": "4.1.5",
        "rimraf": "3.0.0"
    },
    "files": [
        "dist"
    ]
}
