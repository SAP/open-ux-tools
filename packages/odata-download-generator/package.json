        "@types/inquirer": "8.2.6",
        "@types/lodash": "4.14.202",
        "@types/mem-fs": "1.1.2",
        "@types/mem-fs-editor": "7.0.1",
        "@types/vscode": "1.73.1",
        "@types/yeoman-environment": "2.10.11",
        "@types/yeoman-generator": "5.2.14",
        "@types/yeoman-test": "4.0.6",
        "jest-extended": "6.0.0",
        "jest-mock": "30.0.5",
        "mock-spawn": "0.2.6",
        "rimraf": "5.0.5",
        "yeoman-test": "6.3.0"
