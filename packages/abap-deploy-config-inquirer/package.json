        "@types/inquirer-autocomplete-prompt": "2.0.1",
        "@types/lodash": "4.14.202",
        "@types/mem-fs-editor": "7.0.1",
        "@types/mem-fs": "1.1.2",
        "@sap-devx/yeoman-ui-types": "1.14.4",
        "fs-extra": "10.0.0"
    }
