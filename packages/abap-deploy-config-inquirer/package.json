        "@sap-ux/logger": "workspace:*",
        "@sap-ux/project-access": "workspace:*",
        "@sap-ux/project-input-validator": "workspace:*",
        "@sap-ux/store": "workspace:*",
        "@sap-ux/system-access": "workspace:*",
        "@sap-ux/ui5-config": "workspace:*",
        "fast-glob": "3.3.1",
        "lodash": "4.17.21",
        "mem-fs": "2.1.0",
        "mem-fs-editor": "9.4.0",
        "i18next": "23.5.1"
