{
    "sap.ui5": {
        "routing": {
            "targets": {
                "<%- target %>": {
                    "options": {
                        "settings": {
                            "content": {
                                "body": {
                                    "sections": {
                                        "<%- parentSection %>": {
                                            "subSections": {
                                                "<%- name %>": {
                                                    "template": "<%- ns %>.<%- name %>",
                                                    "position": {
                                                        <% if (position.placement) { %> 
                                                            "placement": "<%- position.placement %>", 
                                                        <% } %>
                                                        <% if (position.anchor) { %> 
                                                            "anchor": "<%- position.anchor %>" 
                                                        <% } %>
                                                    },
                                                    "title": "<%- title %>"
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}