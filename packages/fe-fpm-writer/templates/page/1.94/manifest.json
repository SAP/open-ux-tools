{
    "sap.ui5": {
        "routing": {
            "targets": {
                "<%- entity %><%- name %>": {
                    "type": "Component",
                    "id": "<%- entity %><%- name %>",
                    "name": "sap.fe.core.fpm",<%if (locals.controlAggregation) {%>
                    "controlAggregation": "<%- locals.controlAggregation %>",<% } %>
                    "options": {
                        "settings": {
                            "viewName": "<%- ns %>.<%- name %>",
                            "entitySet": "<%- entity %>",
                            "navigation": { }
                        }
                    }
                }<%if (locals.navigation) {%>,
                "<%- navigation.sourcePage %>": {
                    "options": {
                        "settings": {
                            "navigation": {
                                "<%- navigation.navEntity %>": {
                                    "detail": {
                                        "route": "<%- entity %><%- name %>"
                                    }
                                }
                            }
                        }
                    }
                }<% } %>
            }
        }
    }
}