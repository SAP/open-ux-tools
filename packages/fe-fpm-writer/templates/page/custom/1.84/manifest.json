{
    "sap.ui5": {
        "routing": {
            "targets": {
                "<%- entity %><%- name %>": {
                    "viewId": "<%- name %>View",
                    "viewName": "<%- ns %>.<%- name %>",<%if (locals.controlAggregation) {%>
                    "controlAggregation": "<%- locals.controlAggregation %>",<% } %>
                    "options": {
                        "settings": <%- JSON.stringify(settings) %>
                    }
                }<%if (locals.navigation) {%>,
                "<%- navigation.sourcePage %>": {
                    "options": {
                        "settings": {
                            "navigation": {
                                "<%- navigation.navEntity %>": {
                                    "detail": {
                                        "route": "<%- entity %><%- name %>"
                                    }
                                }
                            }
                        }
                    }
                }<% } %>
            }
        }
    }
}