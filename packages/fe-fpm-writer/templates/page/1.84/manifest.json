{
    "sap.ui5": {
        "dependencies": {
            "minUI5Version": "1.84.0"
        },
        "routing": {
            "targets": {
                "<%- entity %><%- id %>": {
					"viewId": "<%- id %>View",
					"viewName": "<%- ns %>.<%- id %>",
					"options": {
						"settings": {
							"entitySet": "<%- entity %>",
							"navigation": { }
						}
					}
				}<%if (locals.navigation) {%>,
                "<%- navigation.sourcePage %>": {
                    "options": {
                        "settings": {
                            "navigation": {
                                "<%- navigation.navEntity %>": {
                                    "detail": {
                                        "route": "<%- entity %><%- id %>"
                                    }
                                }
                            }
                        }
                    }
                }<% } %>
            }
        }
    }
}