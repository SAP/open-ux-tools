{
    "sap.ui5": {
        "routing": {
            "targets": {
                "<%- target %>": {
                    "options": {
                        "settings": {
                            "controlConfiguration": {
                                "<%- targetEntity %>": {
                                    "columns": {
                                        "<%- name %>": {
                                            "header": "<%- header %>",
                                            <% if (locals.width)  { %> 
                                                "width": "<%- width %>",  
                                            <% } %>
                                            "position": <%- JSON.stringify(position) %>,
                                            "template": <% if (typeof fragmentFile !== 'undefined') { %> "<%- ns %>.<%- fragmentFile %>" <% } else { %> "<%- ns %>.<%- name %>" <% } %>
                                            <% if (locals.availability) { %> 
                                                , "availability": "<%- availability %>" 
                                            <% } %>
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}