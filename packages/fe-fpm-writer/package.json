{
    "name": "@sap-ux/fe-fpm-writer",
    "description": "SAP Fiori elements flexible programming model writer",
    "version": "0.24.9",
    "repository": {
        "type": "git",
        "url": "https://github.com/SAP/open-ux-tools.git",
        "directory": "packages/fe-fpm-writer"
    },
    "bugs": {
        "url": "https://github.com/SAP/open-ux-tools/issues?q=is%3Aopen+is%3Aissue+label%3Abug+label%3Afe-fpm-writer"
    },
    "license": "Apache-2.0",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc --build",
        "watch": "tsc --watch",
        "clean": "rimraf --glob dist test/test-output *.tsbuildinfo",
        "format": "prettier --write '**/*.{js,json,ts,yaml,yml}' --ignore-path ../../.prettierignore",
        "lint": "eslint . --ext .ts",
        "lint:fix": "eslint . --ext .ts --fix",
        "test": "jest --ci --forceExit --detectOpenHandles --colors"
    },
    "files": [
        "LICENSE",
        "dist",
        "templates",
        "!dist/*.map",
        "!dist/**/*.map"
    ],
    "dependencies": {
        "@xmldom/xmldom": "0.8.10",
        "ejs": "3.1.9",
        "mem-fs": "2.3.0",
        "mem-fs-editor": "9.7.0",
        "semver": "7.5.4",
        "xml-formatter": "2.6.1",
        "xpath": "0.0.33"
    },
    "devDependencies": {
        "@sap-ux/project-access": "workspace:*",
        "@types/ejs": "3.1.2",
        "@types/mem-fs": "1.1.5",
        "@types/mem-fs-editor": "7.0.7",
        "@types/semver": "7.5.2"
    },
    "engines": {
        "node": ">=18.x"
    }
}
